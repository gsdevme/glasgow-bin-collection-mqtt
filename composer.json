{
    "type": "project",
    "license": "proprietary",
    "prefer-stable": true,
    "require": {
        "php": ">=8",
        "fabpot/goutte": "^4.0",
        "php-mqtt/client": "^1.0",
        "psr/log": "^1.1",
        "symfony/console": "^5",
        "symfony/http-client": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/process": "^5",
        "symfony/var-dumper": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gsdev\\GlasgowWasteCollection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gsdev\\GlasgowWasteCollection\\Tests\\Integration\\": "tests/integration/"
        }
    }
}
